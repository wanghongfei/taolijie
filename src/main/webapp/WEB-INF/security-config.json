{
    "loginUrl": "/login",
    "enableAutoLogin": true,
    "loginCookieName": "token",
    "authBeanName": "defaultAccountService",

    "interceptRule": [
        {
            "roleList": ["ADMIN", "STUDENT" ,"EMPLOYER"],
            "toUrl": "/404",
            "url": "/user**"
        },

        {
            "roleList": ["ADMIN", "EMPLOYER", "STUDENT"],
            "toUrl": "/404",
            "url": "/api/job/u**"
        },
        {
            "roleList": ["ADMIN", "EMPLOYER", "STUDENT"],
            "toUrl": "/404",
            "url": "/api/sh/u**"
        },

        {
            "roleList": ["ADMIN"],
            "toUrl": "/login/admin",
            "url": "/manage**"
        },

        {
            "roleList": ["ADMIN", "STUDENT", "EMPLOYER"],
            "toUrl": "/404",
            "url": "/api/noti**"
        },

        {
            "roleList": ["ADMIN"],
            "toUrl": "/404",
            "url": "/api/recommend/u**"
        }
    ]
}